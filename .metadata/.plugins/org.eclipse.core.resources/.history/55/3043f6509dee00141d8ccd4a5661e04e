	return num;
}

void loadMatrix(int * matrix[][]) {
	int col = 0;
	int row = 0;

	while (row < size) {
		while (col < size) {
			&matrix[row][col++] = random();
			printf("%d ", matrix[row][col]);
		}
		row++;
		printf("\n");
	}
}


int main() {

	int row = 0;

	int matrix1[size][size];
	int matrix2[size][size];

//	int finalMatrix[size][size];
//	int finalMatrix2[size][size];

	loadMatrix(matrix1);
	loadMatrix(matrix2);


	return 0;
}
