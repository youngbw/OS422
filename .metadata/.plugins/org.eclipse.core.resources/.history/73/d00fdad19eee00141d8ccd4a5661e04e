	return num;
}

void loadMatrix(int matrix[][size]) {
	int col = 0;
	int row = 0;

	while (row < size) {
		col = 0;
		while (col < size) {
			matrix[row][col] = random(10);
			printf("%d ", matrix[row][col]);
			col++;
		}
		row++;
		printf("-\n");
	}
}


int main() {

	int row = 0;

	int matrix1[size][size];
	int matrix2[size][size];

//	int finalMatrix[size][size];
//	int finalMatrix2[size][size];

	loadMatrix(matrix1);
	loadMatrix(matrix2);


	return 0;
}
